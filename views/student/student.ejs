<%- include ('./studentHeader') -%>


<style>
    th{
      color: brown;
     
    }
    td{
      color: blue ;
      
    }
  </style>
  
  <h3 class="text-center text-danger pt-5 pb-3">Book-List</h3>
  
  <div class="container">
  
    <table class="table table-borderless">
      <tr>
        <th></th>
        <th>Book Name</th>
        <th>Auther</th>
        <th>Total</th>
        <th>Likes</th>
        <th>Remain</th>
      </tr>
      <% books.forEach(function(entry){ %>
      
        <tr>
          <td> <a href="#"><img src="../images/<%= entry.image %>" alt="" style="width: 50px; border-radius: 50%;"></a></td>
          <td> <a href="/admpage/<%= entry._id %>"><%= entry.bookname %></a></td>
          <td><%= entry.auther %></td>
          <td><%= entry.total %></td>
          <td><%= entry.likes %></td>
          <td><%= entry.remain %></td>
        </tr>
      <% }) %>
    </table>
  </div>
  


<%- include ('../partials/footer') -%>